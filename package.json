  "scripts": {
    "start": "cross-env NODE_ENV=development BABEL_ENV=development styleguidist server --open",
    "styleguide:build": "cross-env NODE_ENV=production BABEL_ENV=production styleguidist build",
    "build": "rimraf dist && cross-env NODE_ENV=production BABEL_ENV=production npm run build:less && rollup -c --inlineDynamicImports",
    "build:watch": "npm run build:less && rollup -c -w",
    "build:less": "node less-bundle.js",
    "test": "jest",
    "test:coverage": "jest --coverage --forceExit --colors",
    "lint": "npm run check-types && npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "eslint": "eslint . --ext .js,.jsx,.tsx,.ts",
    "eslint:fix": "eslint . --ext .js,.jsx,.tsx,.ts --fix",
    "stylelint": "stylelint \"**/*.{css,scss,sass}\"",
    "stylelint:fix": "stylelint \"**/*.{css,scss,sass}\" --fix",
    "check-types": "tsc",
    "prepare": "npm run build",
    "prerelease": "npm run lint:fix && npm run test:coverage",
    "release": "npm publish",
    "predeploy": "npm run styleguide:build",
    "deploy": "gh-pages -d styleguide",
    "generate-fetcher": "restful-react import --config restful-react.config.js",
    "postgenerate-fetcher": "prettier ./src/apis/*.tsx --write",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "commit": "cz"
  },
